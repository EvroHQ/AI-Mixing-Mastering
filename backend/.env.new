# MixMaster Pro Environment Variables

# Backblaze B2 (REQUIRED)
B2_APPLICATION_KEY_ID=your_key_id_here
B2_APPLICATION_KEY=your_app_key_here
B2_BUCKET_INPUT=mixmaster-input
B2_BUCKET_OUTPUT=mixmaster-output

# Redis
REDIS_URL=redis://localhost:6379

# API
ALLOWED_ORIGINS=http://localhost:3000

# Audio Processing
SAMPLE_RATE=48000
MAX_STEMS=12
TARGET_LUFS=-14.0
TRUE_PEAK_LIMIT=-1.0

# Debug
DEBUG=true
